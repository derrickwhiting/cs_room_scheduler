<div class="container">
  <div class="row">
    <div class="col s12 m12 l12">
      <div c></div>
      <ul class="collection">
        <% @days.each do |day| %>
          <%= link_to day_path(day) do %>
           <li class="collection-item"><%= day.date.strftime("%D") %></li>
          <% end %>
            <ul class="collection-item">
            <% day.meetings.where.not(title: nil).order('id asc').each do |meeting| %>
              <%= link_to day_meeting_path(day, meeting) do %>
               <li class="collection-item"><%= meeting.display %>  <%= meeting.title %></li>
              <% end %>
            <% end %>
            </ul>
        <% end %>
      </ul>
      <%= will_paginate @days, class: 'center' %>
    </div>
  </div>
</div>